<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zucc.edu.entity.Staff" table="Staff" schema="LabWork">
        <id name="idStaff" column="idStaff"/>
        <property name="positionStaff" column="positionStaff"/>
        <property name="nameStaff" column="nameStaff"/>
        <property name="passwordStaff" column="passwordStaff"/>
    </class>
</hibernate-mapping>