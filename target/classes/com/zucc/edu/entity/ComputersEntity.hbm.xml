<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zucc.edu.entity.ComputersEntity" table="Computers" schema="LabWork">
        <id name="idComputers" column="idComputers"/>
        <property name="ip" column="ip"/>
        <property name="subnetMask" column="subnetMask"/>
        <property name="fkIdLab" column="fkIdLab"/>
    </class>
</hibernate-mapping>